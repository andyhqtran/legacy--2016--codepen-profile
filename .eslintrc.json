{
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "installedESLint": true,
  "plugins": [
      "react"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1, {
        "extensions": [".js", ".jsx"]
      }
    ],
    "no-underscore-dangle": [
      "error", {
        "allowAfterThis": true
      }
    ]
  }
}